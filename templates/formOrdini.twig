{% extends 'base.twig' %}

{% block subtitle %}- Aggiungi Ordine{% endblock %}

{% block content %}
<form method="post" action="{{ app.urlFor("OrdiniAddPost") }}">
	<label for="IDCliente">IDCliente</label>
	<input type="text" name="idCliente" id="IDCliente"/></br>
	
	<label for="IDProdotto">IDProdotto</label>
	<input type="text" name="idProdotto" id="IDProdotto"/></br>
	
	<label for="DataOrdine">Data Ordine (in formato ISO8601)</label>
	<input type="datetime" name="dataOrdine" id="DataOrdine"><br/>

	<input type="submit" value="Aggiungi Ordine">
	
</form>
{% endblock %}